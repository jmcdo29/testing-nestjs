{
  "name": "testing-nestjs",
  "version": "0.0.1",
  "description": "A repository to show off to the community methods of testing NestJS including Unit Tests, Integration Tests, E2E Tests, pipes, filters, interceptors, GraphQL, Mongo, TypeORM, and more!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmcdo29/testing-nestjs.git"
  },
  "keywords": [
    "Nest",
    "Testing",
    "Jest",
    "Test",
    "Unit",
    "E2E",
    "e2e",
    "integration",
    "NestJS"
  ],
  "author": "Jay McDoniel <jmcdo29@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jmcdo29/testing-nestjs/issues"
  },
  "homepage": "https://github.com/jmcdo29/testing-nestjs#readme",
  "scripts": {
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.(j|t)s(on)?\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"apps/**/*.ts\" --fix",
    "test": "pnpx prisma generate --schema apps/prisma-sample/prisma/schema.prisma && pnpm nx affected:test",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "./setupPrismaDatabase.sh && pnpx prisma generate --schema apps/prisma-sample/prisma/schema.prisma && DATABASE_URL=postgres://postgres:postgres@localhost:5433/prisma nx run-many --target=e2e --all --exclude=prisma-sample",
    "test:cov:total": "DATABASE_URL=postgres://postgres:postgres@localhost:5433/prisma jest -c jest.cov.js",
    "nx": "nx"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.2",
    "@nestjs/axios": "0.0.3",
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.3",
    "@nestjs/cqrs": "^8.0.1",
    "@nestjs/graphql": "^10.0.2",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/microservices": "^8.2.4",
    "@nestjs/mongoose": "^9.0.2",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/platform-fastify": "^8.2.4",
    "@nestjs/platform-socket.io": "^8.2.4",
    "@nestjs/platform-ws": "^8.2.3",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.2",
    "@nestjs/websockets": "^8.2.4",
    "@prisma/client": "^3.8.0",
    "@types/validator": "^13.7.1",
    "apollo-server-core": "^3.6.1",
    "apollo-server-express": "3.6.3",
    "axios": "^0.24.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "express": "^4.17.2",
    "graphql": "^16.3.0",
    "graphql-tools": "^8.2.0",
    "hbs": "^4.1.1",
    "mongoose": "^6.1.6",
    "multer": "^1.4.4",
    "mysql2": "^2.2.5",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "sequelize": "^6.11.0",
    "sequelize-typescript": "^2.1.1",
    "socket.io-client": "^4.4.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.31",
    "webpack": "^5.68.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@golevelup/nestjs-testing": "^0.1.2",
    "@golevelup/ts-jest": "^0.3.2",
    "@nestjs/cli": "^8.1.8",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.4",
    "@nrwl/cli": "^13.4.4",
    "@nrwl/eslint-plugin-nx": "13.10.0",
    "@nrwl/jest": "^13.4.4",
    "@nrwl/linter": "13.7.3",
    "@nrwl/nest": "^13.4.1",
    "@nrwl/node": "^13.4.1",
    "@nrwl/tao": "^13.4.4",
    "@nrwl/workspace": "^13.4.4",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/multer": "^1.4.7",
    "@types/node": "^16",
    "@types/sequelize": "^4.28.11",
    "@types/serve-static": "^1.13.9",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "pactum": "^3.1.1",
    "prettier": "^2.2.1",
    "prisma": "^3.8.0",
    "supertest": "^6.2.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.6",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext ts"
    ],
    "*.{md,html,json,js}": [
      "prettier --write"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "[.]spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
